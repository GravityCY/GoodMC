{
  "schemaVersion": 1,
  "id": "goodmc",
  "version": "${version}",
  "name": "GoodMC",
  "description": "minecraft is now good",
  "authors": [ "GravityIO" ],
  "contact": {
    "sources": "https://github.com/GravityCY/GoodMC",
    "issues": "https://github.com/GravityCY/GoodMC/issues"
  },
  "license": "MIT",
  "icon": "assets/goodmc/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.gravityio.goodmc.GoodMC"
    ],
    "preLaunch": [
      "me.gravityio.goodmc.GoodMC"
    ],
    "client": [
      "me.gravityio.goodmc.client.GoodClientMC"
    ],
    "modmenu": [
      "me.gravityio.goodmc.integration.ModMenuIntegration"
    ],
    "reinfshulker": [
      "me.gravityio.goodmc.integration.ReinforcedShulkersIntegration"
    ]
  },
  "accessWidener": "goodmc.accesswidener",
  "mixins": [
    "goodmc.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fabric_version}",
    "minecraft": "~${minecraft_version}",
    "cloth-config": ">=9.0.94",
    "java": ">=17"
  },
  "recommends": {
    "modmenu": "*"
  }
}
